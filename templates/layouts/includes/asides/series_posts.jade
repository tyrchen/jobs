mixin series_posts(series)
  section
    h1 #{series}
    ul.posts
      - var posts = _.chain(contents.posts).filter(function(item) {
      -   return series == item.metadata.series
      - }).sortBy(function(item) {
      -   // sort them by date
      -   return item.date
      - }).value()
      each post in posts
        li: a(href="#{post.url}") #{post.title}
